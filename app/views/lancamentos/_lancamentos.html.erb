<table class="table table-bordered table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th>Natureza</th>
      <th>Valor R$</th>
      <th>Realizado?</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <% @lancamentos.each do |lancto| %>
    <tr>
      <td><%= lancto.natureza.nome %></td>
      <td><%= lancto.valor %></td>
      <td><input type="checkbox" <%= lancto.realizado ? 'checked' : '' %>></td>
      <td><a class="btn" href="#"><i class="icon-pencil"></i> Editar</a></td>
    </tr>
  <% end %>
  </tbody>
</table>
